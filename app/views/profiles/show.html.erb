<p id="notice"><%= notice %></p>

<% if @post.image.attached? %>
 <p>
 <strong>Image:</strong>
 <br>
 <%= image_tag @post.image %>
 <%= image_tag image.variant(resize: "100x100") %>
 </p>
<% end %>


<p>
  <strong>Title:</strong>
  <%= @profile.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @profile.content %>
</p>

<%= link_to 'Edit', edit_profile_path(@profile) %> |
<%= link_to 'Back', profiles_path %>
